programa
{
/*i) Efetuar a leitura de um valor numérico inteiro e apresentar o resultado do valor lido
elevado ao quadrado.*/
	
	funcao inicio()
{

	     inteiro valor, quadradoValor

		escreva("Digite um valor numérico inteiro: ")
		leia(valor)

		quadradoValor = valor*valor

		escreva("O quadrado de " ,valor, " é: " ,quadradoValor)
	}
}
