programa
{
/*m) Construir um programa que leia três valores numéricos inteiros (representados pelas
variáveis A, B e C) e apresente como resultado final o valor da soma dos quadrados dos
três valores lidos.*/
	
	funcao inicio()
{

	     inteiro a, b, c, somaQuad
	     
		escreva("Digite o primeiro valor: ")
		leia(a)
		escreva("Digite o primeiro valor: ")
		leia(b)
		escreva("Digite o terceiro valor: ")
		leia(c)

		somaQuad = (a*a) + (b*b) + (c*c)

		escreva("A soma dos quadrados dos três números digitados é igual a: " ,somaQuad)
	}
}
